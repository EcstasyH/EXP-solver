(set-logic QF_SLIA)
(set-option :produce-models true)
(declare-fun x () String)
(assert (str.in_re x (re.++ (str.to_re "12345") (re.+ (str.to_re "49")) (re.+ (str.to_re "3"))(str.to_re "6789"))))
(assert (= (mod (mod (str.to_int x) 2417) 2017) 0))
(assert (< (str.len x) 100))
(check-sat)
(get-model)
